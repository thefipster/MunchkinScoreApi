{
  "Serilog": {
    "MinimumLevel": "Verbose",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithExceptionDetails",
      "WithMachineName",
      "WithProcessName",
      "WithAssemblyName",
      "WithAssemblyVersion"
    ]
  },
  "AllowedHosts": [ "http://localhost:4200" ],
  "ConnectionStrings": {
    "StashContext": "stash.litedb"
  }
}
